<ModOps>
  <!-- # Product: Olives -->
  <ModOp Type="addNextSibling" GUID="1010192">
    <Asset>
      <Template>Product</Template>
      <Values>
        <Standard>
          <GUID>1500010105</GUID>
          <Name>Olives</Name>
          <IconFilename>data/products/olives/icon_olives.png</IconFilename>
          <InfoDescription>1500010017</InfoDescription>
        </Standard>
        <Locked />
        <Product>
          <StorageLevel>Building</StorageLevel>
          <ProductCategory>11702</ProductCategory> <!-- natural products -->
          <BasePrice>40</BasePrice>
          <CivLevel>2</CivLevel>
          <AssociatedRegion>Moderate</AssociatedRegion>
          <ProductionRegions>
            <Item>
              <RegionType>Moderate</RegionType>
            </Item>
          </ProductionRegions>
        </Product>
        <ExpeditionAttribute>
          <BaseMorale>10</BaseMorale>
        </ExpeditionAttribute>
      </Values>
    </Asset>
  </ModOp>

  <!-- # Lists: After Grain -->
  <Group>
    <ModOp Type="addNextSibling" GUID="120055"
      Condition="!~/Values/ProductStorageList/ProductList/Item[Product='1500010105']"
      Path="/Values/ProductStorageList/ProductList/Item[Product='1010192']">
      <Item>
        <Product>1500010105</Product>
      </Item>
    </ModOp>
    <ModOp Type="addNextSibling" GUID="502018,502004,502037"
      Condition="!~/Values/ProductList/List/Item[Product='1500010105']"
      Path="/Values/ProductList/List/Item[Product='1010192']">
      <Item>
        <Product>1500010105</Product>
      </Item>
    </ModOp>
  </Group>

  <!-- # Expedition -->
  <ModOp Type="add" GUID="800111"
    Condition="!~/ExpeditionFeature/FeedOption/FeedOptions[Item/Product='1500010105']"
    Path="/Values/ExpeditionFeature/FeedOption/FeedOptions">
    <Item>
      <MoraleFactor>0.4</MoraleFactor>
      <Product>1500010105</Product>
    </Item>
  </ModOp>

  <!-- # Docklands: Tattershire -->
  <Group Condition="!//TradeLevels/Item/Trades[Item/Good='1500010105']">
    <ModOp Type="addNextSibling" GUID="1010192"
      Condition="!//FactoryBase[FactoryOutputs/Item/Product='1500301136']/IsMainFactory[text()='1']">
      <Asset>
        <Template>FarmBuilding</Template>
        <Values>
          <Standard>
            <GUID>1500301136</GUID>
            <Name>IsMainFactory_Fake: Olives</Name>
          </Standard>
          <Building>
            <AssociatedRegions>Moderate</AssociatedRegions>
            <SecondPartyRelevant>0</SecondPartyRelevant>
          </Building>
          <Object>
            <Variations>
              <Item>
                <Filename>data/dlc08/graphics/buildings/production/tree_planter/tree_planter_cherry.cfg</Filename>
              </Item>
            </Variations>
          </Object>
          <Locked />
          <FactoryBase>
            <FactoryOutputs>
              <Item>
                <Product>1500010105</Product>
                <Amount>1</Amount>
                <StorageAmount>2</StorageAmount>
              </Item>
            </FactoryOutputs>
            <CycleTime>60</CycleTime>
            <IsMainFactory>1</IsMainFactory>
          </FactoryBase>
          <Maintenance>
            <Maintenances>
              <Item>
                <Product>1010052</Product>
                <Amount>170</Amount>
              </Item>
            </Maintenances>
          </Maintenance>
          <ModuleOwner>
            <ModuleLimits>
              <Main>
                <Limit>75</Limit>
              </Main>
            </ModuleLimits>
            <ConstructionOptions>
              <Item>
                <ModuleGUID>1010270</ModuleGUID>
              </Item>
            </ConstructionOptions>
          </ModuleOwner>
        </Values>
      </Asset>
    </ModOp>
    <ModOp Type="addNextSibling" GUID="131764"
      Path="/Values/TradeContractFeature/Traders/Item[TraderName='131819']/TradeLevels/Item/Trades/Item[Good='1010198']">
      <Item>
        <Good>1500010105</Good>
      </Item>
    </ModOp>
    <ModOp Type="add" GUID="502233" Path="/Values/ProductList/List">
      <Item>
        <Product>1500010105</Product>
      </Item>
    </ModOp>
  </Group>
</ModOps>