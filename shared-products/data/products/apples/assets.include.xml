<ModOps>
  <!-- # Product: Apples -->
  <ModOp GUID="120014" Type="addNextSibling">
    <Asset>
      <Template>Product</Template>
      <Values>
        <Standard>
          <GUID>1404420032</GUID>
          <Name>Apples</Name>
          <IconFilename>data/products\apples\icon_apples.png</IconFilename>
          <InfoDescription>1404420031</InfoDescription>
        </Standard>
        <Locked />
        <Product>
          <StorageLevel>Building</StorageLevel>
          <ProductCategory>11702</ProductCategory>
          <BasePrice>25</BasePrice>
          <CivLevel>1</CivLevel>
          <AssociatedRegion>Moderate</AssociatedRegion>
        </Product>
        <ExpeditionAttribute>
          <BaseMorale>0</BaseMorale>
          <ExpeditionAttributes>
            <Item>
              <Attribute>Medicine</Attribute>
              <Amount>5</Amount>
            </Item>
          </ExpeditionAttributes>
        </ExpeditionAttribute>
      </Values>
    </Asset>
  </ModOp>

  <!-- # Lists: Before Grapes -->
  <Group>
    <ModOp Type="addPrevSibling" GUID="120055"
      Condition="!~/Values/ProductStorageList/ProductList/Item[Product='1404420032']"
      Path="/Values/ProductStorageList/ProductList/Item[Product='120014']">
      <Item>
        <Product>1404420032</Product>
      </Item>
    </ModOp>

    <ModOp Type="addPrevSibling" GUID="502004,502037"
      Condition="!~/Values/ProductList/List/Item[Product='1404420032']"
      Path="/Values/ProductList/List/Item[Product='120014']">
      <Item>
        <Product>1404420032</Product>
      </Item>
    </ModOp>

    <ModOp Type="add" GUID="502017"
      Condition="!~/Values/ProductList/List/Item[Product='1404420032']"
      Path="/Values/ProductList/List">
      <Item>
        <Product>1404420032</Product>
      </Item>
    </ModOp>
  </Group>

  <!-- # Expedition -->
  <ModOp Type="add" GUID="800111"
    Condition="!~/ExpeditionFeature/FeedOption/FeedOptions[Item/Product='1404420032']"
    Path="/Values/ExpeditionFeature/FeedOption/FeedOptions">
    <Item>
      <MoraleFactor>0.5</MoraleFactor>
      <Product>1404420032</Product>
    </Item>
  </ModOp>


  <!-- # Docklands: Tattershire -->
  <Group Condition="!//TradeLevels/Item/Trades[Item/Good='1404420032']">
    <ModOp Type="addPrevSibling" GUID="120014"
      Condition="!//FactoryBase[FactoryOutputs/Item/Product='1404420032']/IsMainFactory[text()='1']">
      <Asset>
        <Template>FarmBuilding</Template>
        <Values>
          <Standard>
            <GUID>1500301142</GUID>
            <Name>IsMainFactory_Fake: Apples</Name>
          </Standard>
          <Building>
            <AssociatedRegions>Moderate</AssociatedRegions>
            <SecondPartyRelevant>0</SecondPartyRelevant>
          </Building>
          <Object>
            <Variations>
              <Item>
                <Filename>data/dlc08/graphics/buildings/production/tree_planter/tree_planter_cherry.cfg</Filename>
              </Item>
            </Variations>
          </Object>
          <Locked />
          <FactoryBase>
            <FactoryOutputs>
              <Item>
                <Product>1404420032</Product>
                <Amount>1</Amount>
                <StorageAmount>2</StorageAmount>
              </Item>
            </FactoryOutputs>
            <CycleTime>60</CycleTime>
            <IsMainFactory>1</IsMainFactory>
          </FactoryBase>
          <Maintenance>
            <Maintenances>
              <Item>
                <Product>1010052</Product>
                <Amount>35</Amount>
              </Item>
            </Maintenances>
          </Maintenance>
          <ModuleOwner>
            <ModuleLimits>
              <Main>
                <Limit>100</Limit>
              </Main>
            </ModuleLimits>
            <ConstructionOptions>
              <Item>
                <ModuleGUID>1010270</ModuleGUID>
              </Item>
            </ConstructionOptions>
          </ModuleOwner>          
        </Values>
      </Asset>      
    </ModOp>

    <ModOp Type="add" GUID="131764"
      Path="/Values/TradeContractFeature/Traders/Item[TraderName='131819']/TradeLevels/Item/Trades[Item/Good='1010194']">
      <Item>
      	<Level>Uncommon</Level>
        <Good>1404420032</Good>
      </Item>
    </ModOp>

    <ModOp Type="add" GUID="502233" Path="/Values/ProductList/List">
      <Item>
        <Product>1404420032</Product>
      </Item>
    </ModOp>
  </Group>
